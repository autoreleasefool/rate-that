{
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "importOrder": [
    "^@.*$",
    "^assets/(.*)$",
    "^api/(.*)$",
    "^navigation/(.*)$",
    "^sections/(.*)$",
    "^shared/(.*)$",
    "^[.]/(.*)$"
  ]
}
